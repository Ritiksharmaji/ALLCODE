package Generic_method_NPTL;
/*
 * Swapping the integer , dabble , float, String data types
 */
public class Example_2_3_1 {
	
	public static <T> void swapdata(T x, T y)
	{
		T a;
		a = x;
		x = y;
		y = a;
		System.out.println("value of X:"+x+"of Y:"+y);		
	}

	public static void main(String[] args) {
		int a=30,b=50;
		swapdata(a,b);
		System.out.println("before swapping:X:"+);
		float a=(float) 20.9;
		b=49.98;
		swapdata(,);
		swapdata("ritik","sharma");
		swapdata(23.4543,234.09985);

	}

}
